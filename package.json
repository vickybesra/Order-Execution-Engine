{
  "name": "order-execution-engine",
  "version": "1.0.0",
  "description": "High-availability order execution engine with routing and queuing",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "node --localstorage-file=/tmp/jest-localstorage.json node_modules/.bin/jest --watch",
    "test:coverage": "node --localstorage-file=/tmp/jest-localstorage.json node_modules/.bin/jest --coverage"
  },
  "keywords": [
    "order-execution",
    "trading",
    "queue",
    "redis",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/websocket": "^8.3.1",
    "@types/uuid": "^10.0.0",
    "bullmq": "^5.0.0",
    "fastify": "^4.24.3",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "websocket": "^1.0.35",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2"
  }
}
